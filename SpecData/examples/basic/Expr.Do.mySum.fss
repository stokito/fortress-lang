component Expr.Do.mySum

export Executable

(** EXAMPLE **)
mySum(i:ZZ64):ZZ64 = do
  acc:ZZ64 := 0
  for j <- 0:i do
    acc := acc + j
  end
  acc
end
(** END EXAMPLE **)

run(args:String...) = println mySum(3)

end
