component mm64
export Executable

MA32(a:ZZ32[32,32],b:ZZ32[32,32]):ZZ32[32,32] = do
   result:ZZ32[32,32] = matrix[\ZZ32,32,32\]()
   for i<-0#32, j<-0#32 do
      result[i,j] := a[i,j] + b[i,j]
   end
   result
end

