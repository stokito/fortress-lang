MM2(a:ZZ32[2,2],b:ZZ32[2,2]):ZZ32[2,2] = do
   A00:ZZ32 = a[0,0]
   A01:ZZ32 = a[0,1]
   A10:ZZ32 = a[1,0]
   A11:ZZ32 = a[1,1]
   B00:ZZ32 = b[0,0]
   B01:ZZ32 = b[0,1]
   B10:ZZ32 = b[1,0]
   B11:ZZ32 = b[1,1]

   (P0,P1,P2,P3,P4,P5,P6,P7) = (A00 B00,
                                A01 B10,
                                A00 B01,
                                A01 B11,
                                A10 B00,
                                A11 B10,
                                A10 B01,
                                A11 B11)
   (C00,C01,C10,C11) = (P0+P1,P2+P3,P4+P5,P6+P7)
   c:ZZ32[2,2] = [C00 C01;
                  C10 C11]
   c
end

