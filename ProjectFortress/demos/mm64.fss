MM64(a:ZZ32[64,64],b:ZZ32[64,64]):ZZ32[64,64] = do
   A00:ZZ32[32,32] = matrix[\ZZ32,32,32\]()
   A01:ZZ32[32,32] = matrix[\ZZ32,32,32\]()
   A10:ZZ32[32,32] = matrix[\ZZ32,32,32\]()
   A11:ZZ32[32,32] = matrix[\ZZ32,32,32\]()
   B00:ZZ32[32,32] = matrix[\ZZ32,32,32\]()
   B01:ZZ32[32,32] = matrix[\ZZ32,32,32\]()
   B10:ZZ32[32,32] = matrix[\ZZ32,32,32\]()
   B11:ZZ32[32,32] = matrix[\ZZ32,32,32\]()
   for i<-0#32, j<-0#32 do
      A00[i,j] := a[i,j]
      B00[i,j] := b[i,j]
      A01[i,j] := a[i,j+32]
      B01[i,j] := b[i,j+32]
      A10[i,j] := a[i+32,j]
      B10[i,j] := b[i+32,j]
      A11[i,j] := a[i+32,j+32]
      B11[i,j] := b[i+32,j+32]
   end
   (P0,P1,P2,P3,P4,P5,P6,P7) = (MM32(A00,B00),MM32(A01,B10),MM32(A00,B01),
                                MM32(A01,B11),MM32(A10,B11),MM32(A11,B10),
                                MM32(A10,B01),MM32(A11,B11))

   (C00,C01,C10,C11) = (MA32(P0,P1),MA32(P2,P3),MA32(P4,P5),MA32(P6,P7))

   c:ZZ32[64,64] = matrix[\ZZ32,64,64\]()
   for i<-0#32, j<-0#32 do
      c[i,j] := C00[i,j]
      c[i+32,j] := C10[i,j]
      c[i,j+32] := C01[i,j]
      c[i+32,j+32] := C11[i,j]
   end
   c
end

