export Executable

trait a
  m(x:Object) = println("a" x)
end

trait b extends a
  m[\T extends String\](x:T) = println("b" x)
end

object o extends b end

run():() = do
  cat:Object = "cat"
  three:Object = 3
  o.m(cat) (*) Currently wrong -- prints "a cat" not "b cat"
  o.m(three) (*) Current throws and exception
end
