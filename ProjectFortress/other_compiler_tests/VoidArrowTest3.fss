export Executable

hello() : () = println("Hello")
world() : () = println("World!")

trait list[\ T \] 
    getter elt() : T
    getter rest() : list[\T\]
end

object listCell[\T\](head : T, rest : list[\T\]) extends list[\T\] 
    getter elt() : T = head
end

object listEnd[\T\](head : T) extends list[\T\]
    getter elt() : T = head
    getter rest() : list[\T\]  = self
end

apply(x : () -> ()) : () = x ();

run():() = do
    hw : list[\ () -> () \] = listCell(hello,listEnd(world))
    apply(hw.elt)
    w : list[\ () -> () \] = hw.rest
    apply(w.elt)
end
