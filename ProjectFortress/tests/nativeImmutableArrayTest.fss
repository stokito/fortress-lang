component nativeImmutableArrayTest
import NativeArray.{...} (* For PrimImmutableArray *)
export Executable

run(args:String...):() = do
  z:ImmutableArray1[\String, 0, 3\] = PrimImmutableArray[\String, 3\]()
  z.init(0,"cat")
  z.init(1,"dog")
  z.init(2,"bat")
  assert(z[0],"cat","z[0]")
  assert(z[1],"dog","z[1]")
  assert(z[2],"bat","z[2]")
  println( z[0] " " z[1] " " z[2] )
  assert(z.get(1),"dog","z.get(1)")
  println( z.get(1) )
  assert(z.rank(),1,"z.rank()")
  println( z.rank() )
  println(z)
end

end
