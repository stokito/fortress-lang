component litCoercion
export Executable

run(args:String...):()= f()

f(): ZZ64 =
  do
        x: ZZ64 := 0
        y: ZZ32 := 0

        atomic do
                x += 1
                y += 1
        end
        if x=1 AND y=1 then
          println("OK")
        else
          println("FAIL")
        end
   end


end
