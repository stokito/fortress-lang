component XXXoverloadTest4
export Executable

(* This test should fail because of overload between no-type
   and type. *)

var a1c:ZZ32 = 0
var a2c:ZZ32 = 0
var a3c:ZZ32 = 0

a(x)=do
  println("a1 called")
  a1c := 1;
  x
end

a(x:ZZ32)=do
  println("a2 called")
  a2c := 1;
  x
end

a(x, y, z)=do
  println("a3 called")
  a3c := 1;
  x+y+z
end

opr AND(x:Boolean, y:Boolean):Boolean = if x then y else 1=0 end
opr OR(x:Boolean, y:Boolean):Boolean = if x then x=x else y end

run(args:String...):()=do
  println(a(1))

  println(a(1,2))

  println(a(1,2,3))

  println("fail") (* TEST SHOULD NOT RUN *)

end

end
