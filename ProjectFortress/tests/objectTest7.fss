component objectTest7
export Executable

trait T
  getI():ZZ32
  getJ():ZZ32
end

object A(i:ZZ32, j:ZZ32) extends T

     getI():ZZ32 = i
     getJ():ZZ32 = j
     k:ZZ32 = 4

  b(k:ZZ32):T = object extends T
     m:ZZ32 = k
     getI():ZZ32 = i
     getJ():ZZ32 = m
  end


end


run(args:String...):()=do
  a:A = A(1,2)
  b:T = a.b(3)
  println("a.getI() = " a.getI())
  println("a.getJ() = " a.getJ())
  println("b.getI() = " b.getI())
  println("b.getJ() = " b.getJ())

end
end
