component multiGenFor
export Executable

(* Tests that we can write multi-generator for loops *)

run(args:String...):()=do
  a : ZZ32[10,10] = matrix[\ZZ32,10,10\]()
  for i <- 0#10, j <- 10#10 do
    a[i,j-10] := i + i j
  end
  s : ZZ32 := 0
  for i <- seq(0#10), j <- seq(10#10) do
    s += 1
    if a[i,j-10] =/= i + i j then
      fail("FAIL " i "," j " = " a[i,j-10])
    end
  end
  if s =/= 100 then
    fail("FAIL s = " s)
  end
end

end
