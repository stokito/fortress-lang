component exitType
export Executable

trait T
  m(val:ZZ32):ZZ32
end

func(x:ZZ32):ZZ32 = do
   t:T
   _ = label inner
           t = object extends T
                   m(val:ZZ32):ZZ32 = exit with (val + x)
               end
       end inner
   x
end

run(args:String...) = func(3)

end
