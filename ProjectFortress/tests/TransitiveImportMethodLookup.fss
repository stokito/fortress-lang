component TransitiveImportMethodLookup
export Executable

  (* Bug #160 *)

  run(args:String...):() = do
    for ch <- "H" do
        println ch
    end
    typecase ch="Hell" of
        Generator[\Char\] => println "Generator[[Char]]"
        else => println "Something else"
    end

    c : Char = 'd'

    if c MATCH "Hold" then println "It matches!"
    end

    case c of
        "Hold" => println "d is in \"Hold\""
        else => println "d is not in \"Hold\""
    end
  end

end
