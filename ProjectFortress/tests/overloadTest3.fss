component overloadTest3
export Executable

(* This should pass because overloading uses different numbers of arguments,
   even though there is no type (will this remain true when we strictly
   implement the spec?) *)

var a1c:ZZ32 = 0
var a2c:ZZ32 = 0
var a3c:ZZ32 = 0

a(x)=do
  println("a1 called")
  a1c := 1;
  x
end

a(x, y)=do
  println("a2 called")
  a2c := 1;
  x+y
end

a(x, y, z)=do
  println("a3 called")
  a3c := 1;
  x+y+z
end

run(args:String...):()=do
  println(a(1))

  println(a(1,2))

  println(a(1,2,3))

  println(
  if a1c > 0 AND a2c > 0 AND a3c > 0 then "pass" else "fail" end
  )
end

end
