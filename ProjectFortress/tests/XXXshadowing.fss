component XXXshadowing
export Executable

trait Node end
object leaf extends Node end
object node (n1:Node) extends Node
   y = "OK"
   oops (self):node =  do
(*
        n = n1
        typecase n=n of
*)
        typecase n1=n1 of
           leaf => self
           node => self
       end
   end
end

run(args:String...) = println oops(node(leaf)).y

end
