component InheritedOverload
export Executable

trait Base
end

trait Sub1 extends Base excludes {Sub2}
end

trait Sub2 extends Base excludes {Sub1}
end

trait T1
  a(x:Sub1, y:Base):ZZ32 = 2
end

trait T2
  a(x:Base, y:Sub2):ZZ32 = 1
end

object O() extends {T1, T2}
end

run(args:String...):()=do
  o:O = O()
  println("o.b()=" o.b())
end

end
