trait ZZ extends { Number, Equality[\ZZ\] } excludes { RR64, ZZ64, ZZ32, NN32, NN64, IntLiteral }
    coerce(x: IntLiteral) = x.asZZ
    coerce(x: ZZ32) = x.asZZ
