export Executable

trait T
  m(x:String, y:String): String
end

object O(x:ZZ32,y:ZZ32) extends T
  m(a:String, b:String): String = ("O(" x.asString "," y.asString ").m " a b)
end

f(t:T): String = t.m("c","d")

run() = do
  t : T = O(1,2)
  println(t.m("c","d"))
  println(f(t))
end
